{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "no-plusplus": "off",
    "no-bitwise": "off",
    "no-mixed-operators": "off",
    "class-methods-use-this": "off",
    "no-console": "warn",
    "func-names": "off",
    "linebreak-style": "off",
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ]
  }
}